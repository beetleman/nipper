{:style  [:community :justified :respect-nl :extend-nl]
 :fn-map {"defn-" [:arg2
                   {:fn-force-nl #{:arg2}
                    :next-inner  {:remove {:fn-force-nl #{:arg2}}}}]
          "defn"  [:arg2
                   {:fn-force-nl #{:arg2}
                    :next-inner  {:remove {:fn-force-nl #{:arg2}}}}]}
 :map    {:comma? false}}
